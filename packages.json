{
    "dependencies": [
        {
            "link": "https://github.com/microsoft/GSL.git",
            "type": "git",
            "version": "v3.0.0",
            "directory": "gsl",
            "target": "GSL",
            "options": {
                "cmake_variables": {
                    "GSL_TERMINATE_ON_CONTRACT_VIOLATION": "ON",
                    "GSL_MSVC_USE_STL_NOEXCEPTION_WORKAROUND": "ON"
                }
            }
        },
        {
            "link": "https://github.com/matgla/CMakeUtils.git",
            "type": "git",
            "version": "master",
            "directory": "cmake_utils",
            "target": "CMakeUtils",
            "options": {
                "is_cmake_library": true
            }
        }
    ]
}
